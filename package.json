{
  "name": "evaluate",
  "version": "4.0.0",
  "private": true,
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "deploy:commands": "npx ts-node --transpile-only -r tsconfig-paths/register scripts/deploy-commands",
    "deploy:prisma": "npx prisma db push",
    "deploy": "yarn deploy:commands && yarn deploy:prisma",
    "postinstall": "npx patch-package",
    "build:i18n": "npx ts-node --transpile-only -r tsconfig-paths/register scripts/compile-translations",
    "build:prisma": "npx prisma generate",
    "build": "yarn build:i18n && yarn build:prisma && next build --no-lint && yarn deploy",
    "dev:tunnel": "npx localtunnel --port 3000 --subdomain evaluate",
    "dev:i18n": "yarn build:i18n && npx watch-cli -p \"locales/**\" -c \"yarn build:i18n\"",
    "dev:prisma": "npx prisma generate --watch",
    "dev": "yarn dev:i18n & yarn dev:prisma & yarn dev:tunnel & next dev",
    "format": "prettier --write src scripts",
    "lint": "eslint src scripts --ext .ts,.tsx",
    "prepare-for:development": "cp .env.development .env",
    "prepare-for:production": "cp .env.production .env"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.4",
    "@discordjs/core": "^1.0.0",
    "@discordjs/rest": "^2.0.0",
    "@hookform/resolvers": "^3.2.0",
    "@prisma/client": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "autoprefixer": "^10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "discord-api-types": "^0.37.52",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.3.1",
    "fuse.js": "^6.6.2",
    "intl-messageformat": "^10.5.0",
    "lexure": "^0.17.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "next": "^13.4.19",
    "next-sa": "^0.2.0",
    "next-themes": "npm:@wits/next-themes@^0.2.16",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@next/eslint-plugin-next": "^13.4.19",
    "@types/eslint": "^8.44.2",
    "@types/gtag.js": "^0.0.13",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "prisma": "^5.1.1",
    "ts-config-apteryx": "^2.1.0",
    "typescript": "^5.1.6"
  }
}
