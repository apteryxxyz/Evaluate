{
  "extends": "../../tsconfig.react.json",
  "include": ["src/**/*", "next-env.d.ts", ".next/types/**/*.ts"],

  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "strictNullChecks": true,
    "paths": { "~/*": ["./src/*"] }
  }
}
