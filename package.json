{
    "name": "evaluate",
    "version": "3.0.0",
    "private": true,
    "main": "./build/index.js",
    "scripts": {
        "//1": "CHECKING",
        "lint": "npm run lint:types && eslint --ext .ts src",
        "format": "prettier --write \"src/**/*.ts\" && npm run lint -- --fix",
        "lint:types": "tsc --noEmit",
        "//2": "BUILDING",
        "pull": "git reset --hard HEAD && git pull",
        "prepare": "husky install",
        "build": "npm run build:env && npm run build:tsup && npm run build:alias",
        "build:tsup": "tsup --config tsup.config.ts",
        "build:alias": "tsc-alias",
        "build:env": "ts-node ./scripts/build-env.ts",
        "//3": "DATABASE",
        "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/services/Database.ts",
        "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/services/Database.ts -o",
        "//4": "RUNNING",
        "start": "cross-env NODE_ENV=production node build/index.js",
        "start:dev": "npm run build:tsup -- --watch --onSuccess \"npm run build:alias && cross-env NODE_ENV=development node build/index.js\"",
        "//5": "PM2",
        "pm2:start": "pm2 startOrReload pm2.config.js",
        "pm2:stop": "pm2 stop pm2.config.js",
        "pm2:reload": "pm2 startOrReload pm2.config.js",
        "pm2:restart": "pm2 startOrRestart pm2.config.js"
    },
    "dependencies": {
        "@discordjs/builders": "^1.6.1",
        "@vscode/vscode-languagedetection": "^1.0.22",
        "better-sqlite3": "^8.3.0",
        "discord.js": "^14.9.0",
        "dotenv-flow": "^3.2.0",
        "enhanced-ms": "^2.2.0",
        "fuse.js": "^6.6.2",
        "husky": "^8.0.3",
        "lexure": "^0.17.0",
        "lru-cache": "^9.1.1",
        "maclary": "1.0.3-next.26690db.0",
        "openai": "^3.2.1",
        "piston-api-client": "^2.0.1",
        "program-language-detector": "^0.0.12",
        "puppeteer": "^19.11.1",
        "reflect-metadata": "^0.1.13",
        "rentry-pastebin": "^1.3.0",
        "typeorm": "^0.3.15",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@anatine/esbuild-decorators": "^0.2.19",
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/dotenv-flow": "^3.2.0",
        "@types/node": "^18.16.2",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.39.0",
        "eslint-config-apteryx": "^2.0.6",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^43.1.1",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^46.0.0",
        "prettier": "^2.8.8",
        "prettier-config-apteryx": "^2.0.3",
        "ts-config-apteryx": "^2.0.0",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.6",
        "tsup": "^6.7.0",
        "typescript": "^5.0.4"
    },
    "prettier": "prettier-config-apteryx"
}
