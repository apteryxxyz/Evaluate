{
  "name": "@evaluate/translate",
  "description": "Handle translation dictionaries and translating for all of Evaluate.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "biome format src --write",
    "lint": "biome lint src",
    "check": "tsc --noEmit",
    "compile": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/compile/index.ts",
    "build": "pnpm compile && pnpm tsup",
    "dev": "npx watch-cli -p 'dictionaries/*.json' -c 'pnpm compile' & pnpm tsup --watch"
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.7.3",
    "intl-messageformat": "^10.5.8"
  },
  "devDependencies": {
    "@config/tsup": "workspace:^",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1"
  }
}