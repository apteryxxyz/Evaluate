{
  "name": "@evaluate/engine",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "src/index.ts",
  "exports": {
    "./*": {
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*/index.js",
      "types": "./dist/*/index.d.ts"
    }
  },
  "scripts": {
    "format": "biome format . --write",
    "lint": "biome lint . --write",
    "check": "tsc --noEmit",
    "build": "pnpm tsup",
    "dev": "pnpm build --watch"
  },
  "dependencies": {
    "@evaluate/types": "workspace:^",
    "fuse.js": "^7.0.0",
    "lexure": "npm:@apteryxxyz/lexure@^0.17.0",
    "pako": "^2.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@configs/tsup": "workspace:^",
    "@types/node": "^20.16.2",
    "@types/pako": "^2.0.3"
  }
}
