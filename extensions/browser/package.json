{
  "name": "@evaluate/browser-extension",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "biome format . --write",
    "lint": "biome lint .",
    "check": "tsc --noEmit",
    "dev": "plasmo dev",
    "build": "pnpm build:chrome & pnpm build:firefox && pnpm build:edge & pnpm build:opera & pnpm build:brave",
    "build:chrome": "plasmo build --target=chrome-mv3 --zip",
    "build:firefox": "plasmo build --target=firefox-mv3 --zip",
    "build:edge": "plasmo build --target=edge-mv3 --zip",
    "build:opera": "plasmo build --target=opera-mv3 --zip",
    "build:brave": "plasmo build --target=brave-mv3 --zip"
  },
  "dependencies": {
    "@evaluate/compress": "workspace:^",
    "@evaluate/execute": "workspace:^",
    "@evaluate/languages": "workspace:^",
    "@evaluate/react": "workspace:^",
    "@evaluate/translate": "workspace:^",
    "@plasmohq/storage": "1.9.0",
    "framer-motion": "^10.18.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "posthog-js": "npm:@apteryxxyz/posthog-js@^1.104.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.54",
    "autoprefixer": "^10.4.17",
    "plasmo": "^0.84.2",
    "postcss": "^8.4.34",
    "tailwindcss": "^3.4.1"
  },
  "manifest": {
    "name": "Evaluate - Execute Any Code, Any Where",
    "short_name": "Evaluate",
    "description": "Code in any language, anytime, anywhere with Evaluate.",
    "version": "0.0.1",
    "author": "Apteryx Software",
    "homepage_url": "https://evaluate.run",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"]
  }
}
