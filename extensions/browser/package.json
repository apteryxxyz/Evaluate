{
  "name": "@evaluate/browser-extension",
  "version": "0.0.4",
  "type": "module",
  "manifest": {
    "name": "Evaluate - Execute Any Code, Anywhere, Anytime",
    "short_name": "Evaluate",
    "description": "Execute any code snippet on any website with Evaluate.",
    "version": "0.0.4",
    "author": "Apteryx Software",
    "homepage_url": "https://evaluate.run",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"]
  },
  "scripts": {
    "lint": "biome lint . --apply",
    "format": "biome format . --write",
    "check": "tsc --noEmit",
    "dev": "plasmo dev",
    "build:styles": "cp ../../packages/react/style.css ./src/style.css",
    "build": "pnpm build:chrome",
    "build:chrome": "plasmo build --target=chrome-mv3 --zip",
    "package": "plasmo package"
  },
  "dependencies": {
    "@evaluate/engine": "workspace:^",
    "@evaluate/env": "workspace:^",
    "@evaluate/react": "workspace:^",
    "@plasmohq/storage": "^1.11.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@ts-rest/core": "^3.45.2",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^11.2.11",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.338.0",
    "posthog-js": "npm:@apteryxxyz/posthog-js@1.105.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "zod": "3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.263",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "plasmo": "^0.85.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4"
  }
}
