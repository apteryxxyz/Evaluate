{
  "name": "evaluate",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "deploy:commands": "npx ts-node --transpile-only -r tsconfig-paths/register scripts/deploy-commands",
    "deploy:prisma": "npx prisma db push",
    "deploy": "pnpm run \"/^deploy:.*/\"",
    "build:i18n": "npx ts-node --transpile-only -r tsconfig-paths/register scripts/compile-translations",
    "build:prisma": "npx prisma generate",
    "build": "pnpm run \"/^build:.*/\" && next build --no-lint && pnpm run deploy",
    "ui:add": "npx shadcn-ui@latest add -y",
    "dev:tunnel": "npx localtunnel --port 3000 --subdomain evaluate",
    "dev:i18n": "pnpm run build:i18n && npx watch-cli -p \"locales/**\" -c \"pnpm run build:i18n\"",
    "dev:prisma": "npx prisma generate --watch",
    "dev:next": "next dev",
    "dev": "pnpm run \"/^dev:.*/\"",
    "format": "prettier --write src scripts",
    "lint": "eslint src scripts --ext .ts,.tsx",
    "prepare-for:development": "cp .env.development .env",
    "prepare-for:production": "cp .env.production .env"
  },
  "dependencies": {
    "@builders/next": "^0.1.1",
    "@discordjs/builders": "^1.6.5",
    "@discordjs/core": "^1.0.1",
    "@discordjs/rest": "^2.0.1",
    "@formatjs/icu-messageformat-parser": "^2.6.2",
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^5.4.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "discord-api-types": "^0.37.60",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.3.1",
    "fuse.js": "^6.6.2",
    "intl-messageformat": "^10.5.3",
    "lexure": "^0.17.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "next": "^13.5.4",
    "next-themes": "npm:@wits/next-themes@^0.2.16",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@next/bundle-analyzer": "^13.5.4",
    "@next/eslint-plugin-next": "^13.5.4",
    "@types/eslint": "^8.44.3",
    "@types/gtag.js": "^0.0.13",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "prisma": "^5.4.1",
    "ts-config-apteryx": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@discordjs/core@1.0.1": "patches/@discordjs__core@1.0.1.patch"
    }
  }
}
