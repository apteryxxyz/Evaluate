{
    "name": "evaluate",
    "version": "3.0.0",
    "private": true,
    "main": "./build/index.js",
    "scripts": {
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\" && yarn lint --fix",
        "build": "yarn build:env && yarn build:tsup && yarn build:alias",
        "build:env": "ts-node ./scripts/build-env.ts && prettier --write \"src/env.ts\"",
        "build:alias": "tsc-alias",
        "build:tsup": "tsup --config tsup.config.ts",
        "migrate": "npx typeorm migration:run -d ./src/services/Database.ts",
        "migration:generate": "yarn build && npx typeorm migration:generate -d ./src/services/Database.ts -o",
        "start": "cross-env NODE_ENV=production node build/index.js",
        "start:dev": "yarn build:alias && cross-env NODE_ENV=development node build/index.js",
        "dev": "yarn build:tsup --watch --onSuccess \"yarn start:dev\""
    },
    "dependencies": {
        "@discordjs/builders": "^1.6.3",
        "@maclary/lists": "0.3.0-next.776cc52.0",
        "@vscode/vscode-languagedetection": "^1.0.22",
        "better-sqlite3": "^8.3.0",
        "common-tags": "^1.8.2",
        "discord.js": "^14.11.0",
        "dotenv-flow": "^3.2.0",
        "enhanced-ms": "^2.3.0",
        "flourite": "^1.2.3",
        "fuse.js": "^6.6.2",
        "lexure": "^0.17.0",
        "lru-cache": "^9.1.1",
        "maclary": "1.0.3-next.c79ccb7.0",
        "openai": "^3.2.1",
        "piston-api-client": "^2.0.1",
        "puppeteer": "^19.11.1",
        "reflect-metadata": "^0.1.13",
        "rentry-pastebin": "^1.3.0",
        "ts-mixer": "^6.0.3",
        "typeorm": "^0.3.16",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@anatine/esbuild-decorators": "^0.2.19",
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/common-tags": "^1.8.1",
        "@types/dotenv-flow": "^3.2.0",
        "@types/node": "^18.16.7",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.40.0",
        "eslint-config-apteryx": "^2.0.6",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^43.2.0",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^46.0.1",
        "prettier": "^2.8.8",
        "prettier-config-apteryx": "^2.0.3",
        "ts-config-apteryx": "^2.0.0",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.6",
        "tsup": "^6.7.0",
        "typescript": "^5.0.4"
    },
    "prettier": "prettier-config-apteryx",
    "packageManager": "yarn@3.5.1"
}
