{
  "name": "evaluate",
  "version": "4.0.0",
  "private": true,
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "deploy:commands": "npx ts-node --transpile-only -r tsconfig-paths/register cli/commands/deploy",
    "deploy:prisma": "npx prisma db push",
    "deploy": "yarn deploy:commands && yarn deploy:prisma",
    "postinstall": "npx patch-package",
    "build:i18n": "npx ts-node --transpile-only -r tsconfig-paths/register cli/translations/compile",
    "build:prisma": "npx prisma generate",
    "build:contentlayer": "npx contentlayer build",
    "build": "yarn build:i18n && yarn build:contentlayer && yarn build:prisma && next build --no-lint",
    "dev:tunnel": "npx localtunnel --port 3000 --subdomain evaluate",
    "dev:i18n": "yarn build:i18n && npx watch-cli -p \"locales/**\" -c \"yarn build:i18n\"",
    "dev:prisma": "npx prisma studio & npx prisma generate --watch",
    "dev:contentlayer": "npx contentlayer dev",
    "dev": "yarn dev:i18n & yarn dev:contentlayer & yarn dev:prisma & yarn dev:tunnel & next dev",
    "format": "prettier --write src cli",
    "lint": "eslint src cli --ext .ts",
    "prepare-for:development": "cp .env.development .env",
    "prepare-for:production": "cp .env.production .env"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.4",
    "@discordjs/core": "^1.0.0",
    "@discordjs/rest": "^2.0.0",
    "@prisma/client": "^5.1.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "autoprefixer": "^10.4.14",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "discord-api-types": "^0.37.52",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.3.1",
    "fuse.js": "^6.6.2",
    "intl-messageformat": "^10.5.0",
    "lexure": "^0.17.0",
    "lodash": "^4.17.21",
    "next": "^13.4.13",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@next/eslint-plugin-next": "^13.4.13",
    "@types/eslint": "^8.44.2",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.1.1",
    "ts-config-apteryx": "^2.1.0",
    "typescript": "^5.1.6"
  }
}
