{
  "name": "evaluate",
  "packageManager": "pnpm@9.3.0",
  "workspaces": ["apps/*", "bots/*", "extensions/*", "packages/*"],
  "scripts": {
    "lint": "biome lint . --write",
    "format": "biome format . --write",
    "check": "pnpm turbo --concurrency=50 check",
    "build": "pnpm turbo --concurrency=50 build",
    "build:packages": "pnpm turbo --concurrency=50 --filter=./packages/* build",
    "build:apps": "pnpm turbo --concurrency=50 build",
    "start": "pnpm turbo --concurrency=50 start",
    "dev": "pnpm turbo --concurrency=50 dev",
    "dev:packages": "pnpm turbo --concurrency=50 --filter=./packages/* dev",
    "dev:apps": "pnpm turbo --concurrency=50 dev",
    "test": "pnpm check && pnpm turbo --concurrency=50 test",
    "pretty": "pnpm lint && pnpm format",
    "graph": "pnpm dlx nx@latest graph"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.16.2",
    "resolve-tspaths": "^0.8.19",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "turbo": "^2.1.0",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "zod@3.22.4": "patches/zod@3.22.4.patch"
    }
  }
}
