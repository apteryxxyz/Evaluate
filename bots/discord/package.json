{
  "name": "@evaluate/discord-bot",
  "version": "1.0.0",
  "description": "The Discord bot version of Evaluate.",
  "type": "module",
  "bin": {
    "deploy-commands": "./scripts/deploy-commands.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "biome format . --write",
    "lint": "biome lint .",
    "check": "tsc --noEmit",
    "build": "tsup",
    "dev": "pnpm build --watch"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/core": "^1.1.1",
    "@discordjs/rest": "^2.2.0",
    "@evaluate/capture": "workspace:^",
    "@evaluate/compress": "workspace:^",
    "@evaluate/execute": "workspace:^",
    "@evaluate/languages": "workspace:^",
    "@evaluate/translate": "workspace:^",
    "@next/env": "^14.1.0",
    "discord-api-types": "^0.37.70",
    "discord-interactions": "^3.4.0",
    "lodash": "^4.17.21",
    "posthog-node": "^3.6.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@configs/tsup": "workspace:^",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2"
  }
}
