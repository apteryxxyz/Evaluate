{
  "name": "@evaluate/translate",
  "description": "Handle translation dictionaries and translating for all of Evaluate.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "format": "biome format . --write",
    "lint": "biome lint .",
    "check": "tsc --noEmit",
    "compile": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/compile/index.ts",
    "build": "pnpm compile && pnpm tsup",
    "dev": "pnpm run \"/^dev:.*/\"",
    "dev:build": "pnpm build --watch",
    "dev:watch": "npx watch-cli -p \"dictionaries/*.json\" -c \"pnpm compile\""
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": "^2.7.6",
    "@formatjs/intl-localematcher": "^0.5.4",
    "intl-messageformat": "^10.5.11"
  },
  "devDependencies": {
    "@configs/tsup": "workspace:^",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2"
  }
}
