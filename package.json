{
  "name": "evaluate",
  "packageManager": "pnpm@9.14.4",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "pretty": "biome check . --write",
    "check": "turbo run check",
    "build": "turbo run build",
    "build:packages": "turbo run build --filter=\"./packages/*\"",
    "dev": "pnpm run \"/dev:.*/\"",
    "dev:packages": "turbo watch build --filter=\"./packages/*\"",
    "dev:apps": "turbo run dev --filter=\"./apps/*\"",
    "graph": "pnpx nx@18 graph"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.17.9",
    "resolve-tspaths": "^0.8.23",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "zod@3.22.4": "patches/zod@3.22.4.patch",
      "posthog-js@1.161.3": "patches/posthog-js@1.161.3.patch"
    }
  }
}
