{
    "name": "evaluate",
    "version": "2.0.0",
    "description": "",
    "license": "MIT",
    "main": "dist/src/index.js",
    "packageManager": "yarn@3.2.1",
    "scripts": {
        "build": "yarn build:clean && tsc && tsc-alias",
        "build:clean": "rimraf dist",
        "build:watch": "concurrently \"tsc -w\" \"tsc-alias -w\"",
        "format": "eslint {src,lib} --fix && prettier src lib --write",
        "dev": "cross-env NODE_ENV=development && node .",
        "dev:watch": "nodemon --watch dist --exec yarn dev",
        "pull": "git reset --hard HEAD && git pull",
        "serve": "cross-env NODE_ENV=production node .",
        "start": "pm2 startOrReload pm2.config.json",
        "stop": "pm2 stop pm2.config.json",
        "update": "yarn pull && yarn build && yarn start"
    },
    "dependencies": {
        "@discordjs/builders": "^1.1.0",
        "@maclary/context": "^0.1.3",
        "@vscode/vscode-languagedetection": "^1.0.21",
        "common-tags": "^1.8.2",
        "discord.js": "dev",
        "dotenv": "^16.0.1",
        "enhanced-ms": "^1.0.2",
        "fuse.js": "^6.6.2",
        "lexure": "^0.17.0",
        "lodash": "^4.17.21",
        "maclary": "dev",
        "mongoose": "^6.4.1",
        "node-cache": "^5.1.2",
        "piston-api-client": "^2.0.1",
        "program-language-detector": "^0.0.12"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.1",
        "@types/lodash": "^4.14.182",
        "apteryx-eslint-config": "^1.0.0",
        "apteryx-prettier-config": "^1.0.1",
        "concurrently": "^7.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.18.0",
        "mongodb-memory-server": "^8.7.2",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "tsc-alias": "^1.7.0"
    }
}
